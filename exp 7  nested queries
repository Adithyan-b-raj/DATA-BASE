2.SELECT COUNT(*) FROM EMPLOYEE11 WHERE SALARY > (SELECT AVG(SALARY) FROM EMPLOYEE11);

  COUNT(*)
----------
         1

3.SELECT * FROM PROJECT11 WHERE PNO = (SELECT PNO FROM EMPLOYEE11 WHERE SALARY = (SELECT MAX(SALARY) FROM EMPLOYEE11));

PNO                  PNAME                    DEPTNO
-------------------- -------------------- ----------
P2                   Project2                      2

4.select deptname from employee11,project11,dept11 where employee11.pno=project11.pno and project11.deptno=dept11.deptno and age=(select min(age) from employee11);

