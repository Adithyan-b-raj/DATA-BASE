10.select * from employee11 where eno=(SELECT eno FROM EMPLOYEE11,PROJECT11,DEPT11 WHERE EMPLOYEE11.Pno=PROJECT11.pno and Project11.DEPTNO=DEPT11.DEPTNO and DEPTNAME = 'CSE') ORDER BY ENAME;

