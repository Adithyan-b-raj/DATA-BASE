-- 1
SELECT COUNT(DISTINCT ENO) FROM PROJECT WHERE PNO NOT IN (SELECT DISTINCT PNO FROM PROJECT WHERE ENO IN (SELECT ENO FROM EMPLOYEE WHERE ENAME IN ('Praveen', 'Aruna')));

-- 2
SELECT COUNT(*) FROM EMPLOYEE WHERE SALARY > (SELECT AVG(SALARY) FROM EMPLOYEE);

-- 3
SELECT * FROM PROJECT WHERE ENO IN (SELECT ENO FROM EMPLOYEE WHERE SALARY = (SELECT MAX(SALARY) FROM EMPLOYEE));

-- 4
SELECT DEPT FROM EMPLOYEE WHERE DOB = (SELECT MAX(DOB) FROM EMPLOYEE);

-- 5
SELECT * FROM EMPLOYEE WHERE EXTRACT(MONTH FROM DOJ) = 1;

-- 6
SELECT TO_CHAR(DOJ, 'Month') AS JOIN_MONTH, COUNT(*) AS EMP_COUNT FROM EMPLOYEE GROUP BY TO_CHAR(DOJ, 'Month');

-- 7
UPDATE EMPLOYEE SET DEPT = 'UpdatedDept' WHERE EXTRACT(MONTH FROM DOJ) = 3 AND SALARY > (SELECT AVG(SALARY) FROM EMPLOYEE);

-- 8
UPDATE EMPLOYEE SET SALARY = SALARY * 1.10 WHERE DOJ = (SELECT MIN(DOJ) FROM EMPLOYEE);

-- 9
SELECT * FROM EMPLOYEE WHERE ENO IN (SELECT ENO FROM PROJECT WHERE PNO NOT IN (SELECT PNO FROM PROJECT WHERE ENO = (SELECT ENO FROM EMPLOYEE WHERE ENAME = 'Arun')));

-- 10
SELECT * FROM EMPLOYEE WHERE DEPT = 'CSE' ORDER BY ENAME ASC;
